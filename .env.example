# Docker Compose environment for NextGenTeamBlue
# Copy to .env and fill in real values

# Google Cloud SQL instance connection name: project:region:instance
INSTANCE_CONNECTION_NAME=your-project:region:your-instance

# Path to your service account key (JSON) on the host machine
# This path is volume-mounted into the backend container as /app/gcp-key.json
GCP_KEY_PATH=./env/gcp-key.json

# Database credentials
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=teamblue-asset-ms
